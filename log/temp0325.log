import os
import shutil

def move_files(source_dir, keyword, destination_folder):
    """
    Moves text files containing a specific keyword from a source directory to a destination folder.
    
    Args:
    source_dir (str): The directory to search for text files.
    keyword (str): The keyword to search for within the text files.
    destination_folder (str): The destination folder where matching files will be moved to.
    
    Returns:
    str: A message indicating the completion of the task and how many files were moved.
    """
    # Ensure the destination folder exists, create if it doesn't
    destination_path = os.path.join(source_dir, destination_folder)
    if not os.path.exists(destination_path):
        os.makedirs(destination_path)
    
    moved_files_count = 0
    # Iterate through all files in the source directory
    for filename in os.listdir(source_dir):
        file_path = os.path.join(source_dir, filename)
        # Check if the current file is a text file
        if os.path.isfile(file_path) and filename.endswith('.txt'):
            # Open and read the file to search for the keyword
            with open(file_path, 'r', encoding='utf-8') as file:
                if keyword in file.read():
                    # Move file to the destination folder if keyword is found
                    shutil.move(file_path, destination_path)
                    moved_files_count += 1
    
    return f"Moved {moved_files_count} files containing the keyword '{keyword}' to '{destination_folder}'."

